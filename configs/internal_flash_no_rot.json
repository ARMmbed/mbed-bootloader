{
    "macros": [
        "MBED_CLOUD_CLIENT_UPDATE_STORAGE=ARM_UCP_FLASHIAP"
    ],
    "target_overrides": {
        "*": {
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-flush-at-exit": false,
            "update-client.firmware-header-version": "2",
            "mbed-bootloader.use-kvstore-rot": 0
        },
        "K64F": {
            "update-client.application-details"        : "(32*1024)",
            "mbed-bootloader.application-start-address": "(33*1024)",
            "mbed-bootloader.max-application-size"     : "((512-33)*1024)",
            "update-client.storage-address"            : "((512+48)*1024)",
            "update-client.storage-size"               : "((512-48)*1024)",
            "update-client.storage-locations"          : 1,
            "update-client.storage-page"               : 8
        },
        "K66F": {
            "update-client.application-details"        : "(32*1024)",
            "mbed-bootloader.application-start-address": "(33*1024)",
            "mbed-bootloader.max-application-size"     : "((1024-33)*1024)",
            "update-client.storage-address"            : "((1024+100)*1024)",
            "update-client.storage-size"               : "((1024-100)*1024)",
            "update-client.storage-locations"          : 1,
            "update-client.storage-page"               : 8
        },
        "NUCLEO_F429ZI": {
            "update-client.application-details"        : "(0x08000000+32*1024)",
            "mbed-bootloader.application-start-address": "(0x08000000+33*1024)",
            "mbed-bootloader.max-application-size"     : "((1024-65)*1024)",
            "update-client.storage-address"            : "(0x08000000+(1024+128)*1024)",
            "update-client.storage-size"               : "((1024-128)*1024)",
            "update-client.storage-locations"          : 1,
            "update-client.storage-page"               : 1
        },
        "NUCLEO_F303RE": {
            "update-client.application-details"        : "(0x08000000+32*1024)",
            "mbed-bootloader.application-start-address": "(0x08000000+33*1024)",
            "mbed-bootloader.max-application-size"     : "((256-33)*1024)",
            "update-client.storage-address"            : "(0x08000000+(256+48)*1024)",
            "update-client.storage-size"               : "((256-48)*1024)",
            "update-client.storage-locations"          : 1,
            "update-client.storage-page"               : 4
        },
        "NRF52_DK": {
            "minimal-printf.console-output"            : "SWO",
            "target.app_offset"                        : "0x74000",
            "update-client.application-details"        : "(508*1024)",
            "mbed-bootloader.application-start-address": "(4*1024)",
            "mbed-bootloader.max-application-size"     : "(232*1024)",
            "update-client.storage-address"            : "(236*1024)",
            "update-client.storage-size"               : "(228*1024)",
            "update-client.storage-locations"          : 1,
            "update-client.storage-page"               : 4,
            "target.extra_labels_remove"               : ["SOFTDEVICE_S132_FULL", "SOFTDEVICE_COMMON"],
            "target.extra_labels_add"                  : ["SOFTDEVICE_S132_MBR"]
        }
    }
}
