            "mbed-bootloader.application-start-address": "(FLASH_START_ADDRESS+65*1024)"
        },
        "UBLOX_C030_U201": {
            "update-client.application-details"        : "(FLASH_START_ADDRESS+64*1024)",
            "mbed-bootloader.application-start-address": "(FLASH_START_ADDRESS+65*1024)"
        }
    }
